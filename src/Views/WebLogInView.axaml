<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="clr-namespace:AI_Prompt_Editor.ViewModels"
             xmlns:views="clr-namespace:AI_Prompt_Editor.Views"
             mc:Ignorable="d"
             d:DesignWidth="800"
             d:DesignHeight="450"
             x:Class="AI_Prompt_Editor.Views.WebLogInView">
    <Grid VerticalAlignment="Stretch">
        <Border BorderBrush="{DynamicResource ControlStrokeColorDefaultBrush}"
                BorderThickness="1">
            <Decorator x:Name="WebLogInBrowserWrapper"
                       Focusable="False"
                       IsVisible="{Binding WebLoginViewIsVisible}" />
        </Border>
    </Grid>
</UserControl>